{
  "extends": [
    "config:base"
  ],
  "automerge": true,
  "constraints": {
    "python": "~=3.9.2"
  },
  "lockFileMaintenance": {
    "enabled": true
  },
  "pip-compile": {
    "fileMatch": [
      "(^|/)requirements\\.in$"
    ]
  },
  "pip_requirements": {
    "enabled": false
  },
  "platformAutomerge": true
}
